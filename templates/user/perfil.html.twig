    <script>
        $( function() {
            var ciudad= $(".jg_ciudad").val();
            var provincia=$(".jg_provincia").val();
            //alert(ciudad);
            var aciudad= $.ajax({
                dataType: "json",
                url:"/public/json/municipios.json",
                success:function(resultado){
                    $.each(resultado,function(i,value){
                        if(value.municipio_id == ciudad){
                            $(".jg_resul_city").append(value.nombre);
                            $(".jg_ciudad").hide();
                        }

                    })

                },
                error:function(){
                    //si por ejemplo, est√° mal el nombre del fichero,
                    //saltar√° el error
                    $(".jg_resul_city").html("se ha producido un error");
                }
            });

            var aprovincia= $.ajax({
                dataType: "json",
                url:"/public/json/provincias.json",
                success:function(resultado){
                    $.each(resultado,function(i,value){
                        if(value.provincia_id == provincia){
                            $(".jg_resul_province").append(value.nombre);
                            $(".jg_provincia").hide();
                        }

                    })

                },
                error:function(){
                    //si por ejemplo, est√° mal el nombre del fichero,
                    //saltar√° el error
                    $(".jg_resul_province").html("se ha producido un error");
                }
            });

        });
    </script>
